{
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "dev:candidate": {
      "dependsOn": ["candidate#dev", "moderator#dev"],
      "cache": false
    },
    "dev:landing": {
      "dependsOn": ["landing#dev", "moderator#dev"],
      "cache": false
    },
    "dev:socialtrend": {
      "dependsOn": ["socialtrend#dev", "moderator#dev"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": {
      "outputs": []
    }
  }
}
